#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/josehower/upleveled-projects/test-eslint/node_modules/.pnpm/eslint-config-upleveled@4.0.12_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.0_@t_j6wv2sslgdxd2jcxq47syyczyy/node_modules/eslint-config-upleveled/bin/node_modules:/home/josehower/upleveled-projects/test-eslint/node_modules/.pnpm/eslint-config-upleveled@4.0.12_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.0_@t_j6wv2sslgdxd2jcxq47syyczyy/node_modules/eslint-config-upleveled/node_modules:/home/josehower/upleveled-projects/test-eslint/node_modules/.pnpm/eslint-config-upleveled@4.0.12_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.0_@t_j6wv2sslgdxd2jcxq47syyczyy/node_modules:/home/josehower/upleveled-projects/test-eslint/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/josehower/upleveled-projects/test-eslint/node_modules/.pnpm/eslint-config-upleveled@4.0.12_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.0_@t_j6wv2sslgdxd2jcxq47syyczyy/node_modules/eslint-config-upleveled/bin/node_modules:/home/josehower/upleveled-projects/test-eslint/node_modules/.pnpm/eslint-config-upleveled@4.0.12_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.0_@t_j6wv2sslgdxd2jcxq47syyczyy/node_modules/eslint-config-upleveled/node_modules:/home/josehower/upleveled-projects/test-eslint/node_modules/.pnpm/eslint-config-upleveled@4.0.12_@babel+eslint-parser@7.21.3_@next+eslint-plugin-next@13.3.0_@t_j6wv2sslgdxd2jcxq47syyczyy/node_modules:/home/josehower/upleveled-projects/test-eslint/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint-config-upleveled/bin/install.js" "$@"
else
  exec node  "$basedir/../eslint-config-upleveled/bin/install.js" "$@"
fi
